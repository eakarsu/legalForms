    <!-- FAQ Schema -->
    <script type="application/ld+json">
    {
      "@context": "https://schema.org",
      "@type": "FAQPage",
      "mainEntity": [
        {
          "@type": "Question",
          "name": "Are the generated documents legally binding?",
          "acceptedAnswer": {
            "@type": "Answer",
            "text": "Our documents are based on legally compliant templates, but all generated documents should be reviewed by a qualified attorney before use. Laws vary by jurisdiction."
          }
        },
        {
          "@type": "Question", 
          "name": "What document formats are available?",
          "acceptedAnswer": {
            "@type": "Answer",
            "text": "We offer documents in PDF, Microsoft Word (.docx), and plain text formats to meet your specific needs."
          }
        },
        {
          "@type": "Question",
          "name": "How much does it cost?",
          "acceptedAnswer": {
            "@type": "Answer", 
            "text": "Our service is currently free to use. We may introduce premium features in the future."
          }
        },
        {
          "@type": "Question",
          "name": "Are documents state-specific?",
          "acceptedAnswer": {
            "@type": "Answer",
            "text": "Yes, our templates are designed to comply with state-specific legal requirements across all 50 states."
          }
        }
      ]
    }
    </script>
